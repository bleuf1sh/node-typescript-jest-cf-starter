{
  "name": "node-typescript-jest-cf-starter",
  "version": "0.0.1",
  "description": "Starter skeleton for Node using typescript and Cloud Foundry and using jest for tests",
  "author": "bleuf1sh",
  "license": "MIT",
  "main": "app.ts",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "test": "ts-node node_modules/jest/bin/jest.js",
    "dev": "npm run clean && ts-node-dev --respawn --transpileOnly src/app.ts",
